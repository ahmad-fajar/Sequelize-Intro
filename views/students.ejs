<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Students</title>
  </head>
  <body>
    <h1>Students</h1>

    <a href="/students/addstudent">Add New Student</a>
    <br>
    <br>

    <table>
      <tr>
        <th>ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Options</th>
      </tr>

    <% data.forEach(list => { %>
      <tr>
        <td><%= list.id %></td>
        <td><%= list.first_name %></td>
        <td><%= list.last_name %></td>
        <td><%= list.email %></td>
        <td>
          <a href="/students/edit/<%= list.id %>">Edit</a>
          <a href="/students/delete/<%= list.id %>">Delete</a>
        </td>
      </tr>
    <% }); %>

    </table>

  </body>
</html>