<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Teachers</title>
  </head>
  <body>
    <h1>Teachers</h1>
    <%= data[0].first_name %>

    <table>
      <tr>
        <th>ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
      </tr>

    <% data.forEach(list => { %>
      <tr>
        <td><%= list.id %></td>
        <td><%= list.first_name %></td>
        <td><%= list.last_name %></td>
        <td><%= list.email %></td>
      </tr>
    <% }); %>

    </table>

  </body>
</html>